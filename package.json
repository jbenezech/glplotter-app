{
  "name": "glplotter-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "./react",
  "dependencies": {
    "@craco/craco": "^6.4.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.12",
    "@mui/styles": "^5.10.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.2.2",
    "formik": "^2.2.9",
    "glplotter": "github:jbenezech/glplotter",
    "i18next": "^22.0.4",
    "notistack": "^2.0.8",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "prebuild": "yarn run lint",
    "format": "npx prettier --write . \"{**/*,*}.{js,ts,jsx,tsx,css,scss,sass}\"",
    "lint": "npx eslint src/ --ext .js,.jsx,.ts,.tsx --ignore-pattern node_modules/ && npx stylelint \"**/*.scss\"",
    "lint-format": "npm run format && npm run lint",
    "prepare": "husky install",
    "precommit": "yarn pretty-quick --staged && yarn dlx eslint src/ --ext .js,.jsx,.ts,.tsx --ignore-pattern node_modules/ --cache --fix && yarn dlx stylelint \"**/*.scss\" --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/reactcss": "^1.2.6",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "craco-alias": "^3.0.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "latest",
    "husky": "^8.0.1",
    "postcss": "^8.4.19",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.56.0",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "github:jbenezech/stylelint-config-sass-guidelines#fix/postcss-yarn",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.8.4"
  }
}
