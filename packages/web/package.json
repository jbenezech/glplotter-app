    "eslint-plugin-react-hooks": "latest",
    "husky": "^8.0.1",
    "postcss": "^8.4.19",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.56.0",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.3",
