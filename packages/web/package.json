    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@glplotter-app/common": "*",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "@mui/styles": "*",
    "@testing-library/jest-dom": "*",
