    "react-dom": "^18.2.0"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepack": "yarn build",
    "build": "yarn clean && yarn compile",
    "compile": "cross-env webpack && tsc -b -v",
    "watch": "concurrently -n \"Webpack,TypeScript\" -c \"cyan.bold,blue.bold\" \"webpack -w\" \"tsc --build -v --pretty --watch --preserveWatchOutput\" -k",
    "prebuild": "yarn run lint",
    "format": "npx prettier --write . \"{**/*,*}.{js,ts,jsx,tsx,css,scss,sass}\"",
    "lint": "npx eslint src/ --ext .js,.jsx,.ts,.tsx --ignore-pattern node_modules/ && npx stylelint \"**/*.scss\"",
    "lint-format": "npm run format && npm run lint",
    "precommit": "yarn pretty-quick --staged && yarn dlx eslint src/ --ext .js,.jsx,.ts,.tsx --ignore-pattern node_modules/ --cache --fix && yarn dlx stylelint \"**/*.scss\" --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/reactcss": "^1.2.6",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "babel-loader": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "latest",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mini-css-extract-plugin": "^2.7.0",
    "postcss": "^8.4.19",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "sass": "^1.56.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "styled-jsx": "^5.1.0",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "github:jbenezech/stylelint-config-sass-guidelines#fix/postcss-yarn",
    "stylelint-config-standard": "^29.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
