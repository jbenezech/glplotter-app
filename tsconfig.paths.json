{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Pages/*": ["src/pages/*"],
      "@Components/*": ["src/components/*"],
      "@Icons/*": ["src/components/svg/icons/*"],
      "@Pictos/*": ["src/components/svg/pictos/*"],
      "@Svg/*": ["src/components/svg/*"],
      "@Styles/*": ["src/styles/*"],
      "@Utils/*": ["src/utils/*"],
      "@Services/*": ["src/services/*"],
      "@Validation/*": ["src/validation/*"],
      "@Router/*": ["src/router/*"],
      "@Constants": ["src/Constants.ts"],
      "@Theme": ["src/Theme.tsx"],
      "@I18n": ["src/I18n.tsx"],
      "@Types": ["src/types.ts"],
      "@Hooks/*": ["src/hooks/*"],
      "@Context/*": ["src/context/*"]
    }
  }
}
